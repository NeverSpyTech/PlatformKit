<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyVersion>2.0.0.7</AssemblyVersion>
    <FileVersion>2.0.0.7</FileVersion>
    <Version>2.0.0-alpha7</Version>
    <Authors>AluminiumTech</Authors>
    <Company />
    <Description>AluminiumCoreLib is designed to help programmers be more efficient in C# by containing things that programmers should have and/or may need in their application.</Description>
    <PackageLicenseUrl></PackageLicenseUrl>
    <Copyright>Copyright (c) AluminiumTech 2017-2020</Copyright>
    <RepositoryUrl>https://github.com/AluminiumTech/AluminiumCoreLib</RepositoryUrl>
    <PackageReleaseNotes>## Changes
* Moved existing Process related functionality to new ProcessManager class
* Removed SecureRNG to its own library
* Added the ability to terminate processes
* Added String process name to Process object translation method
* Removed AluminiumCoreLib.IO
* Removed old testing project</PackageReleaseNotes>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>